{
  "name": "nodejs-sample-app",
  "version": "0.0.1",
  "description": "NodeJS Sample App",
  "author": "Kabir Baidhya <kabirbaidhya@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "run-s lint start:dev",
    "start:dev": "nodemon --exec babel-node src/server/index.js",
    "lint": "eslint .",
    "clean": "rimraf dist/",
    "build": "run-s lint clean babel",
    "babel": "babel src/ -d dist/",
    "list:routes": "babel-node ./resources/scripts/list-routes.js",
    "migrate": "knex migrate:latest",
    "migrate:make": "knex migrate:make",
    "migrate:rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "seed:make": "knex seed:make"
  },
  "dependencies": {
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "bookshelf": "^0.10.2",
    "camelize": "^1.0.0",
    "cors": "^2.8.1",
    "express": "^4.14.0",
    "winston": "^2.3.0",
    "dotenv": "^4.0.0",
    "knex": "^0.13.0",
    "morgan": "^1.7.0",
    "pg": "^6.1.5"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^5.0.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-es2015": "^6.18.0",
    "eslint": "2.2.0",
    "express-list-routes": "^0.1.4",
    "foreman": "^2.0.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.1.1",
    "rimraf": "^2.5.4"
  }
}
