{
  "name": "nodejs-sample-app",
  "version": "0.0.1",
  "description": "NodeJS Sample App",
  "author": "Kabir Baidhya <kabirbaidhya@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "run-s lint start:dev",
    "lint": "eslint .",
    "clean": "rimraf dist/ public/build",
    "build": "run-s lint clean build:all",
    "build:all": "./resources/scripts/build.sh",
    "build:prod": "NODE_ENV=production npm run build",
    "list:routes": "babel-node ./resources/scripts/list-routes.js",
    "start:dev": "run-p start:b start:f",
    "start:b": "nodemon --exec babel-node src/server/index.js",
    "start:f": "webpack-dev-server --config resources/webpack/dev.config.js --colors",
    "migrate": "knex migrate:latest",
    "migrate:make": "knex migrate:make",
    "migrate:rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "seed:make": "knex seed:make"
  },
  "dependencies": {
    "angular": "^1.5.8",
    "angular-resource": "^1.5.8",
    "angular-ui-block": "^0.0.3-0",
    "angular-ui-router": "^0.3.2",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "bookshelf": "^0.10.2",
    "camelize": "^1.0.0",
    "cors": "^2.8.1",
    "express": "^4.14.0",
    "jquery": "^3.1.1",
    "keycode-js": "^0.0.3",
    "winston": "^2.3.0",
    "dotenv": "^4.0.0",
    "knex": "^0.13.0",
    "morgan": "^1.7.0",
    "pg": "^6.1.5"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-es2015": "^6.18.0",
    "eslint": "2.2.0",
    "express-list-routes": "^0.1.4",
    "foreman": "^2.0.0",
    "json-loader": "^0.5.4",
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.1.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  }
}
